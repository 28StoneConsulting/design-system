    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "auto": "^10.37.6",
    "chromatic": "^6.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react-scripts": "5.0.1",
